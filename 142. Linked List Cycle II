##Better Solution

public class Solution { // TC = O(2N)  SC = O(N)
    public ListNode detectCycle(ListNode head) {
        HashMap<ListNode , Integer> map = new HashMap<>();
        ListNode temp = head ;
        while(temp != null){
            if(map.containsKey(temp)){
                return temp ;
            }
            map.put(temp,1);
            temp = temp.next ;
        }
        return null;
    }
}


##Optimize soltion
public class Solution { //TC = O(N)  SC = O(1)
    public ListNode detectCycle(ListNode head) {
        ListNode slow = head ;
        ListNode fast = head ;
        while(fast != null && fast.next != null){
            slow = slow.next ;
            fast = fast.next.next ;
            
            if(slow == fast){
                slow = head ;
                while(slow != fast){
                    slow = slow.next ;
                    fast = fast.next ;
                }
                return slow ; // or fast ;
            }
        }
        return null;
    }
}
